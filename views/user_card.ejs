<div class="card m-3" style="width: 18rem;">
    <img src="<%= user[0].img %>" class="card-img-top" alt="...">
    <div class="card-body">
        <h5 class="card-title">
            <%= user[0].f_name %>
                <%= user[0].l_name %>
        </h5>
        <p class="card-text">Username : <%= user[0].username %>
        </p>
        <p class="card-text">Password : <%= user[0].password %>
        </p>

    </div>
</div>

